            "fontSize": { "value": 16, "unit": "px" },
            "fontWeight": "{lui.brand.typography.weight.subtitle}",
            "lineHeight": "{lui.brand.typography.line-height.heading}",
            "letterSpacing": "{lui.typography.tracking.default}"
          }
        },
        "block-title": {
          "$value": {
            "fontFamily": "{lui.brand.typography.font-family.heading}",
            "fontSize": { "value": 16, "unit": "px" },
            "fontWeight": "{lui.brand.typography.weight.block-title}",
            "lineHeight": "{lui.brand.typography.line-height.heading}",
            "letterSpacing": "{lui.typography.tracking.default}"
          }
        },
        "subheadline": {
          "$value": {
            "fontFamily": "{lui.brand.typography.font-family.heading}",
            "fontSize": { "value": 16, "unit": "px" },
            "fontWeight": "{lui.brand.typography.weight.subheadline}",
            "lineHeight": "{lui.brand.typography.line-height.heading}",
            "letterSpacing": "{lui.typography.tracking.default}"
          }
        },
        "overtitle": {
          "$value": {
            "fontFamily": "{lui.brand.typography.font-family.heading}",
            "fontSize": { "value": 16, "unit": "px" },
            "fontWeight": "{lui.brand.typography.weight.overtitle}",
            "lineHeight": "{lui.brand.typography.line-height.heading}",
            "letterSpacing": "{lui.typography.tracking.default}",
            "textCase": "{lui.typography.case.uppercase}"
          }
        },
        "body": {
          "lg": {
            "$value": {
              "fontFamily": "{lui.brand.typography.font-family.body}",
              "fontSize": { "value": 16, "unit": "px" },
              "fontWeight": "{lui.typography.weight.regular}",
              "lineHeight": "{lui.brand.typography.line-height.body}",
              "letterSpacing": "{lui.typography.tracking.default}"
            }
          },
          "md": {
            "$value": {
              "fontFamily": "{lui.brand.typography.font-family.body}",
              "fontSize": { "value": 16, "unit": "px" },
              "fontWeight": "{lui.typography.weight.regular}",
              "lineHeight": "{lui.brand.typography.line-height.body}",
              "letterSpacing": "{lui.typography.tracking.default}"
            }
          },
          "sm": {
            "$value": {
              "fontFamily": "{lui.brand.typography.font-family.body}",
              "fontSize": { "value": 16, "unit": "px" },
              "fontWeight": "{lui.typography.weight.regular}",
              "lineHeight": "{lui.brand.typography.line-height.body}",
              "letterSpacing": "{lui.typography.tracking.default}"
            }
          }
        }
      }
    }
  }
