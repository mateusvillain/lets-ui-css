{
  "lui": {
    "border": {
      "$type": "dimension",
      "width": {
          "0": {
            "$value": { "value": 0, "unit": "px" }
          },
          "1": {
            "$value": { "value": 1, "unit": "px" }
          },
          "2": {
            "$value": { "value": 2, "unit": "px" }
          }
      }
    },
    "spacing": {
      "fixed": {
        "$type": "dimension",
          "0": {
            "$value": { "value": 0, "unit": "px" }
          },
          "8": {
            "$value": { "value": 8, "unit": "px" }
          },
          "16": {
            "$value": { "value": 16, "unit": "px" }
          },
          "24": {
            "$value": { "value": 24, "unit": "px" }
          },
          "32": {
            "$value": { "value": 32, "unit": "px" }
          },
          "40": {
            "$value": { "value": 40, "unit": "px" }
          }
      },
      "fluid": {
          "$type": "string",
          "2": {
            "$value": "clamp(0.1125rem, 0.109rem + 0.001vw, 0.125rem)"
          },
          "4": {
            "$value": "clamp(0.225rem, 0.218rem + 0.002vw, 0.25rem)"
          },
          "8": {
            "$value": "clamp(0.45rem, 0.436rem + 0.004vw, 0.5rem)"
          },
          "12": {
            "$value": "clamp(0.675rem, 0.655rem + 0.006vw, 0.75rem)"
          },
          "16": {
            "$value": "clamp(0.9rem, 0.874rem + 0.008vw, 1rem)"
          },
          "20": {
            "$value": "clamp(1.125rem, 1.093rem + 0.01vw, 1.25rem)"
          },
          "24": {
            "$value": "clamp(1.35rem, 1.311rem + 0.012vw, 1.5rem)"
          },
          "32": {
            "$value": "clamp(1.8rem, 1.748rem + 0.016vw, 2rem)"
          },
          "40": {
            "$value": "clamp(2.25rem, 2.186rem + 0.02vw, 2.5rem)"
          },
          "48": {
            "$value": "clamp(2.7rem, 2.623rem + 0.024vw, 3rem)"
          },
          "56": {
            "$value": "clamp(3.15rem, 3.061rem + 0.028vw, 3.5rem)"
          },
          "64": {
            "$value": "clamp(3.6rem, 3.498rem + 0.032vw, 4rem)"
          },
          "72": {
            "$value": "clamp(4.05rem, 3.936rem + 0.036vw, 4.5rem)"
          },
          "80": {
            "$value": "clamp(4.5rem, 4.374rem + 0.04vw, 5rem)"
          }
      }
    },
    "opacity": {
        "$type": "number",
        "0": {
          "$value": 0
        },
        "100": {
          "$value": 1
        }
    },
    "typography": {
      "weight": {
          "$type": "fontWeight",
          "light": {
            "$value": 300
          },
          "regular": {
            "$value": 400
          },
          "medium": {
            "$value": 500
          },
          "semibold": {
            "$value": 600
          },
          "bold": {
            "$value": 700
          }
      },
      "line-height": {
          "$type": "number",
          "lg": {
            "$value": 1.5
          },
          "md": {
            "$value": 1.2
          },
          "sm": {
            "$value": 1.125
          }
      },
      "tracking": {
          "$type": "dimension",
          "tightest": {
            "$value": { "value": -2, "unit": "px" }
          },
          "tighter": {
            "$value": { "value": -1, "unit": "px" }
          },
          "default": {
            "$value": { "value": 0, "unit": "px" }
          },
          "wider": {
            "$value": { "value": 1, "unit": "px" }
          },
          "widest": {
            "$value": { "value": 2, "unit": "px" }
          }
      },
      "case": {
          "$type": "string",
          "none": {
            "$value": "none"
          },
          "uppercase": {
            "$value": "uppercase"
          },
          "lowercase": {
            "$value": "lowercase"
          }
      },
      "decoration": {
          "$type": "string",
          "none": {
            "$value": "none"
          },
          "underline": {
            "$value": "underline"
          },
          "line-through": {
            "$value": "line-through"
          }
      }
    },
    "grid": {
      "column": {
        "$type": "number",
        "lg": {
          "$value": 12
        },
        "md": {
          "$value": 8
        },
        "sm": {
          "$value": 4
        }
      },
      "breakpoint": {
        "$type": "dimension",
        "lg": {
          "$value": { "value": 1280, "unit": "px" }
        },
        "md": {
          "$value": { "value": 720, "unit": "px" }
        }
      }
    }
  }
}
