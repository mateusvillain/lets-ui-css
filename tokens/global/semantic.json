{
  "lui": {
    "border": {
      "radius": {
        "none": {
          "$value": { "value": 0, "unit": "px" },
          "$type": "dimension"
        },
        "1xs": {
          "$value": "{lui.brand.border.radius.1xs}"
        },
        "sm": {
          "$value": "{lui.brand.border.radius.sm}"
        },
        "md": {
          "$value": "{lui.brand.border.radius.md}"
        },
        "lg": {
          "$value": "{lui.brand.border.radius.lg}"
        },
        "circle": {
          "$value": { "value": 1000000, "unit": "px" },
          "$type": "dimension"
        }
      }
    },
    "color": {
      "info": {
        "background": {
          "surface": {
            "$value": "{lui.color.blue.1}"
          },
          "container": {
            "$value": "{lui.color.blue.5}"
          }
        },
        "border": {
          "stroke": {
            "$value": "{lui.color.blue.5}"
          }
        },
        "icon": {
          "default": {
            "$value": "{lui.color.blue.5}"
          }
        }
      },
      "danger": {
        "background": {
          "surface": {
            "$value": "{lui.color.red.1}"
          },
          "container": {
            "$value": "{lui.color.red.5}"
          }
        },
        "border": {
          "stroke": {
            "$value": "{lui.color.red.5}"
          }
        },
        "text": {
          "error": {
            "$value": "{lui.color.red.5}"
          }
        },
        "icon": {
          "default": {
            "$value": "{lui.color.red.5}"
          }
        }
      },
      "success": {
        "background": {
          "surface": {
            "$value": "{lui.color.green.1}"
          },
          "container": {
            "$value": "{lui.color.green.5}"
          }
        },
        "border": {
          "stroke": {
            "$value": "{lui.color.green.5}"
          }
        },
        "icon": {
          "default": {
            "$value": "{lui.color.green.5}"
          }
        }
      },
      "caution": {
        "background": {
          "surface": {
            "$value": "{lui.color.orange.1}"
          },
          "container": {
            "$value": "{lui.color.orange.5}"
          }
        },
        "border": {
          "stroke": {
            "$value": "{lui.color.orange.5}"
          }
        },
        "icon": {
          "default": {
            "$value": "{lui.color.orange.5}"
          }
        }
      },
      "neutral": {
        "background": {
          "surface": {
            "$value": "{lui.color.gray.1}"
          },
          "container": {
            "$value": "{lui.color.gray.3}"
          },
          "overlay": {
            "$value": "{lui.color.gray.9}"
          }
        },
        "border": {
          "divisor": {
            "$value": "{lui.color.gray.2}"
          },
          "stroke": {
            "$value": "{lui.color.gray.5}"
          }
        },
        "text": {
          "heading": {
            "$value": "{lui.color.gray.9}"
          },
          "body": {
            "$value": "{lui.color.gray.8}"
          },
          "inverse": {
            "$value": "{lui.color.gray.1}"
          }
        },
        "icon": {
          "default": {
            "$value": "{lui.color.gray.9}"
          },
          "inverse": {
            "$value": "{lui.color.gray.1}"
          }
        }
      },
      "accent": {
        "background": {
          "surface": {
            "$value": "{lui.brand.color.1}"
          },
          "container": {
            "$value": "{lui.brand.color.5}"
          }
        },
        "border": {
          "stroke": {
            "$value": "{lui.brand.color.5}"
          }
        },
        "icon": {
          "default": {
            "$value": "{lui.brand.color.5}"
          }
        }
      },
      "link": {
        "default": {
          "$value": "{lui.brand.color.5}"
        },
        "visited": {
          "$value": "{lui.brand.color.7}"
        }
      }
    },
    "opacity": {
      "hidden": {
        "$value": "{lui.opacity.0}"
      },
      "disabled": {
        "$value": "{lui.brand.opacity.disabled}"
      },
      "visible": {
        "$value": "{lui.opacity.100}"
      }
    },
    "elevation": {
      "$type": "shadow",
      "base": {
        "$value": [
          {
            "offsetX": { "value": 0, "unit": "px" },
            "offsetY": { "value": 0, "unit": "px" },
            "blur": { "value": 8, "unit": "px" },
            "spread": { "value": 0, "unit": "px" },
            "color": {
                "colorSpace": "srgb",
                "components": [0, 0, 0],
                "alpha": 0.02,
                "hex": "#000000"
            }
          },
          {
            "offsetX": { "value": 0, "unit": "px" },
            "offsetY": { "value": 4, "unit": "px" },
            "blur": { "value": 12, "unit": "px" },
            "spread": { "value": 0, "unit": "px" },
            "color": {
                "colorSpace": "srgb",
                "components": [0, 0, 0],
                "alpha": 0.04,
                "hex": "#000000"
            }
          }
        ]
      },
      "hovered": {
        "$value": [
          {
            "offsetX": { "value": 0, "unit": "px" },
            "offsetY": { "value": 0, "unit": "px" },
            "blur": { "value": 8, "unit": "px" },
            "spread": { "value": 0, "unit": "px" },
            "color": {
                "colorSpace": "srgb",
                "components": [0, 0, 0],
                "alpha": 0.04,
                "hex": "#000000"
            }
          },
          {
            "offsetX": { "value": 0, "unit": "px" },
            "offsetY": { "value": 8, "unit": "px" },
            "blur": { "value": 16, "unit": "px" },
            "spread": { "value": 0, "unit": "px" },
            "color": {
                "colorSpace": "srgb",
                "components": [0, 0, 0],
                "alpha": 0.08,
                "hex": "#000000"
            }
          }
        ]
      },
      "floating": {
        "$value": [
          {
            "offsetX": { "value": 0, "unit": "px" },
            "offsetY": { "value": 0, "unit": "px" },
            "blur": { "value": 8, "unit": "px" },
            "spread": { "value": 0, "unit": "px" },
            "color": {
                "colorSpace": "srgb",
                "components": [0, 0, 0],
                "alpha": 0.08,
                "hex": "#000000"
            }
          },
          {
            "offsetX": { "value": 0, "unit": "px" },
            "offsetY": { "value": 12, "unit": "px" },
            "blur": { "value": 20, "unit": "px" },
            "spread": { "value": 0, "unit": "px" },
            "color": {
                "colorSpace": "srgb",
                "components": [0, 0, 0],
                "alpha": 0.12,
                "hex": "#000000"
            }
          }
        ]
      },
      "overlay": {
        "$value": [
          {
            "offsetX": { "value": 0, "unit": "px" },
            "offsetY": { "value": 1, "unit": "px" },
            "blur": { "value": 8, "unit": "px" },
            "spread": { "value": 0, "unit": "px" },
            "color": {
                "colorSpace": "srgb",
                "components": [0, 0, 0],
                "alpha": 0.04,
                "hex": "#000000"
            }
          },
          {
            "offsetX": { "value": 0, "unit": "px" },
            "offsetY": { "value": 16, "unit": "px" },
            "blur": { "value": 24, "unit": "px" },
            "spread": { "value": 0, "unit": "px" },
            "color": {
                "colorSpace": "srgb",
                "components": [0, 0, 0],
                "alpha": 0.16,
                "hex": "#000000"
            }
          }
        ]
      }
    },
    "typography": {
      "$type": "typography",
      "scale": {
        "display": {
          "$value": {
            "fontFamily": "{lui.brand.typography.font-family.heading}",
            "fontSize": { "value": 16, "unit": "px" },
            "fontWeight": "{lui.brand.typography.weight.display}",
            "lineHeight": "{lui.brand.typography.line-height.heading}",
            "letterSpacing": "{lui.typography.tracking.default}"
          }
        },
        "title": {
          "$value": {
            "fontFamily": "{lui.brand.typography.font-family.heading}",
            "fontSize": { "value": 16, "unit": "px" },
            "fontWeight": "{lui.brand.typography.weight.title}",
            "lineHeight": "{lui.brand.typography.line-height.heading}",
            "letterSpacing": "{lui.typography.tracking.default}"
          }
        },
        "headline": {
          "$value": {
            "fontFamily": "{lui.brand.typography.font-family.heading}",
            "fontSize": { "value": 16, "unit": "px" },
            "fontWeight": "{lui.brand.typography.weight.headline}",
            "lineHeight": "{lui.brand.typography.line-height.heading}",
            "letterSpacing": "{lui.typography.tracking.default}"
          }
        },
        "subtitle": {
          "$value": {
